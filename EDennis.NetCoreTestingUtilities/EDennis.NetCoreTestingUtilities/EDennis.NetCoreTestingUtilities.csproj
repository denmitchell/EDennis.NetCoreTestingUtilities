<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Dennis Mitchell</Authors>
    <Company>edennis.com</Company>
    <Description>Provides various classes that assist with .NET Core 2 unit testing.</Description>
    <Summary>Provides various classes that assist with .NET Core 2 unit testing.</Summary>
    <PackageTags>Json Testing JXML XML</PackageTags>
    <PackageProjectUrl>https://github.com/denmitchell/NetCoreTestingUtilities</PackageProjectUrl>
    <PackageIconUrl>https://avatars0.githubusercontent.com/u/2938333?s=400&amp;v=4</PackageIconUrl>
    <RepositoryUrl>https://github.com/denmitchell/NetCoreTestingUtilities</RepositoryUrl>
    <Version>2.3.7</Version>
    <PackageReleaseNotes>Added an ArgumentException when a max depth of 1 is used with a collection of objects during calls to ToJsonString, IsEqual, IsEqualOrWrite ... etc.  The max depth for a collection of objects must be at least 2.</PackageReleaseNotes>
    <AssemblyVersion>2.3.7</AssemblyVersion>
    <FileVersion>2.3.7</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="EDennis.JsonUtils" Version="0.2.3" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="2.0.1" />
    <PackageReference Include="JsonDiffPatch" Version="2.0.45" />
    <PackageReference Include="xunit.abstractions" Version="2.0.1" />
  </ItemGroup>
  
  </Project>
